        <input type="radio" id="Eclairage-bon" name="Eclairage" value="bon">
        <label for="Eclairage-bon">Bon</label>
        <input type="radio" id="Eclairage-mal" name="Eclairage" value="mal">
        <label for="Eclairage-mal">Mal</label><br>

        <label>Klaxon</label>
