        <input type="radio" id="Gyrophare-bon" name="Gyrophare" value="bon">
        <label for="Gyrophare-bon">Bon</label>
        <input type="radio" id="Gyrophare-mal" name="Gyrophare" value="mal">
        <label for="Gyrophare-mal">Mal</label><br>

